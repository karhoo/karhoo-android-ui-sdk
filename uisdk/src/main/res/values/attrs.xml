    <declare-styleable name="CategorySelectorView">
        <attr name="selectedTabTextColor" format="reference" />
        <attr name="unselectedTabTextColor" format="reference" />
        <attr name="unavailableTabTextColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="BookingPaymentView">
        <attr name="addPaymentBackground" format="reference" />
        <attr name="changePaymentBackground" format="reference" />
        <attr name="addCardIcon" format="reference" />
        <attr name="lineText" format="reference" />
        <attr name="actionText" format="reference" />
    </declare-styleable>

    <declare-styleable name="BookingPriceView">
        <attr name="supplierHeaderText" format="reference" />
        <attr name="supplierDetailsText" format="reference" />
    </declare-styleable>

    <declare-styleable name="BookingSupplierView">
        <attr name="headerText" format="reference" />
        <attr name="detailsText" format="reference" />
    </declare-styleable>

    <declare-styleable name="BookingTermsView">
        <attr name="termsText" format="reference" />
        <attr name="termsLinkText" format="reference" />
    </declare-styleable>

