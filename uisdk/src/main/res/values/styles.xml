<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="ResourceName">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/kh_uisdk_primary</item>
        <item name="colorPrimaryDark">@color/kh_uisdk_primary</item>
        <item name="colorAccent">@color/kh_uisdk_accent</item>
        <item name="colorControlNormal">@color/kh_uisdk_med_grey</item>
        <item name="colorControlActivated">@color/kh_uisdk_primary</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:actionMenuTextColor">@color/kh_uisdk_colorAccent</item>
        <item name="android:windowSharedElementEnterTransition">
            @transition/uisdk_change_image_transform
        </item>
        <item name="android:windowSharedElementExitTransition">
            @transition/uisdk_change_image_transform
        </item>
        <item name="fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/kh_uisdk_text_secondary</item>
        <item name="android:textColorHint">@color/kh_uisdk_search_hint_color</item>
        <item name="hintTextColor">@color/kh_uisdk_search_hint_color</item>
        <item name="colorError">@color/kh_uisdk_error</item>
    </style>

    <style name="AppTheme.Transparent" parent="AppTheme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="SplashTheme" parent="AppTheme">
        <item name="android:windowBackground">@drawable/uisdk_background_splash</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="colorPrimaryDark">@color/kh_uisdk_transparent</item>
        <item name="android:statusBarColor">@color/kh_uisdk_transparent</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/kh_uisdk_primary</item>
        <item name="colorControlNormal">@color/kh_uisdk_secondary</item>
        <item name="android:textColor">@color/kh_uisdk_dark_grey</item>
        <item name="colorControlActivated">@color/kh_uisdk_accent</item>
        <item name="materialThemeOverlay">@style/DialogButtonTheme</item>
    </style>

    <!-- Quick fix for date picker text buttons -->
    <style name="DialogButtonTheme">
        <item name="colorOnPrimary">@color/kh_uisdk_secondary</item>
        <item name="colorOnSecondary">@color/kh_uisdk_secondary</item>
        <item name="colorOnSurface">@color/kh_uisdk_secondary</item>
    </style>

    <!-- Quick fix for no background buttons -->
    <style name="NoBackgroundButtonTheme">
        <item name="colorOnSurface">@color/kh_uisdk_off_white</item>
        <item name="colorPrimary">@color/kh_uisdk_off_white</item>
        <item name="background">@color/kh_uisdk_off_white</item>
        <item name="backgroundTint">@color/kh_uisdk_off_white</item>
        <item name="android:textColor">@color/uisdk_text_action</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="AppTheme">
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.OffWhiteProgressBar">
        <item name="colorAccent">@color/kh_uisdk_off_white</item>
        <item name="android:alpha">0.7</item>
    </style>

    <style name="AppTheme.Toolbar">
        <item name="colorControlNormal">@color/kh_uisdk_off_white</item>
        <item name="android:textColorSecondary">@color/kh_uisdk_off_white</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:background">@color/kh_uisdk_primary</item>
    </style>

    <style name="AppTheme.Overflow" parent="AppTheme">
        <item name="actionMenuTextColor">@color/kh_uisdk_off_black</item>
    </style>

    <style name="AppTheme.Recycler" parent="AppTheme">
        <item name="android:colorEdgeEffect">@color/kh_uisdk_off_white</item>
    </style>

    <style name="AppTheme.TransparentActionBar" parent="AppTheme">
        <item name="colorPrimary">@color/kh_uisdk_primary</item>
        <item name="android:background">@drawable/uisdk_gradient_toolbar</item>
        <item name="android:popupElevation">@dimen/kh_uisdk_elevation_default</item>
    </style>

    <style name="AppTheme.TransparentActionBar.Theme" parent="AppTheme">
        <item name="colorPrimary">@color/kh_uisdk_primary</item>
        <item name="colorControlNormal">@color/kh_uisdk_primary</item>
        <item name="android:textColorSecondary">@color/kh_uisdk_primary</item>
        <item name="android:textColorPrimary">@color/kh_uisdk_primary</item>
    </style>

    <style name="AppTheme.EditText" parent="AppTheme">
        <item name="android:textSize">@dimen/kh_uisdk_text_size_medium</item>
        <item name="colorAccent">@color/kh_uisdk_primary</item>
        <item name="colorControlNormal">@color/kh_uisdk_dark_grey</item>
        <item name="colorControlActivated">@color/kh_uisdk_primary</item>
        <item name="colorControlHighlight">@color/kh_uisdk_primary</item>
    </style>

    <style name="AppTheme.EditTextWithHint" parent="AppTheme.EditText">
