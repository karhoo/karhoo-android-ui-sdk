        <item name="colorAccent">@color/kh_uisdk_primary</item>
        <item name="colorControlNormal">@color/kh_uisdk_med_grey</item>
        <item name="colorControlActivated">@color/kh_uisdk_primary</item>
        <item name="colorControlHighlight">@color/kh_uisdk_primary</item>
    </style>

    <style name="AdyenCheckout.PrimaryFlatButton" parent="AdyenCheckout">
        <item name="android:buttonStyle">@style/Widget.AppCompat.Button.Borderless.Colored</item>
        <item name="colorAccent">@color/kh_uisdk_secondary</item>
    </style>

    <style name="AdyenCheckout.SecondaryFlatButton" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:textColor">@color/kh_uisdk_secondary</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <declare-styleable name="CustomTextInputLayout">
        <attr name="helperText" format="string" />
